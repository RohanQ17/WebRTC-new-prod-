{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.js" },
    { "src": "/send_message", "dest": "/api/send_message.js" },
    { "src": "/get_messages", "dest": "/api/get_messages.js" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
